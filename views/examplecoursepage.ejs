<%- include("./partials/header")%>

    <h1><%= course.name %> </h1>

    <div class="list-group my-list">
      <% course.labs.forEach(lab => { %>
         <a href="#" class="list-group-item list-group-item-action"><%= lab.name %> </a>
      <% }) %>
        <!-- <a href="#" class="list-group-item list-group-item-action">Lab 1</a>
        <a href="#" class="list-group-item list-group-item-action">Lab 2</a>
        <a href="#" class="list-group-item list-group-item-action">Lab 3</a> -->
      </div>
      <% var newCourse='/courses/'+course._id+'/new'; %> 
      <h2>Lab not found? Add it <a href='<%= newCourse %> '>here.</a></h2>
      <a href="/courses/<%= course._id %>/edit" class = "btn btn-warning">Edit</a>
      <form action="/courses/<%= course._id %>?_method=DELETE" method="POST" id="delete">
        <button type="submit" class="btn btn-danger">Delete Course</button>
      </form>
<%- include("./partials/footer")%>